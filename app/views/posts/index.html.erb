	<h1 id='page_title'>My posts</h1>

	<div id='post_wrapper' class='skinny_wrapper'>
		<% @post.each do |post| %>
		<div class='post'>
			<p class= 'date'><%= post.created_at.strftime("%A, %b %d") %></p>
			<h2><a href="#"><%= link_to post.title, post %> </a></h2>
			<hr>
		</div>
		<% end %>
		</div>
<!-- paginations -->



<%= will_paginate @post %>


